{
  "name": "@switchboard-xyz/on-demand",
  "version": "3.7.3",
  "description": "A Typescript client to interact with Switchboard On-Demand.",
  "license": "ISC",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": {
    "os": false,
    "fs": false,
    "path": false,
    "events": "events",
    "@coral-xyz/anchor-31/dist/cjs/nodewallet": "./dist/esm/shims/nodewallet.js"
  },
  "types": "dist/types/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "pnpm lint",
    "build": "tsc && tsc --project tsconfig.cjs.json",
    "build:browser": "webpack --config webpack.config.js",
    "check-types": "tsc --noEmit",
    "docgen": "typedoc --options ./typedoc.json",
    "docgen:deploy": "pnpm docgen && firebase deploy --project switchboard-docs --only hosting:switchboard-docs",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "prepack": "pnpm build",
    "test": "pnpm exec jest --passWithNoTests"
  },
  "dependencies": {
    "@coral-xyz/anchor-31": "npm:@coral-xyz/anchor@0.31.1",
    "@isaacs/ttlcache": "^1.4.1",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "@switchboard-xyz/common": "^5.2.9",
    "axios": "^1.9",
    "bs58": "^6.0.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "isomorphic-ws": "^5.0.0",
    "js-yaml": "^4.1.0",
    "tweetnacl": "^1.0.3",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@the-ruby-group/rgts": "0.1.0",
    "@types/node": "latest",
    "@types/ws": "^8.18.0",
    "babel-loader": "^10.0.0",
    "crypto-browserify": "^3.12.1",
    "eslint": "^9",
    "gts": "^6.0.2",
    "https-browserify": "^1.0.0",
    "jest": "^29.7.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.2",
    "typescript": "^5",
    "url": "^0.11.4",
    "webpack": "^5.100.1",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": ">= 18"
  }
}
